<!-- dynamic brand -->

<div class="brand-dynamic">
    <svg style="overflow:visible" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" width="387px" height="84px" viewBox="0 0 387 84" enable-background="new 0 0 387 84" xml:space="preserve">
        <defs>
            <style type="text/css">
                <![CDATA[.draw-line {
                    fill:transparent;
                    stroke:url(#myLinearGradient1);
                    stroke-width:1;
                }
                .fill {
                    fill:#ffffff;
                    opacity:0;
                }
                ]]>
            </style>
            <linearGradient id="myLinearGradient1" x1="0%" y1="0%" x2="0%" y2="100%" spreadMethod="pad">
                <stop offset="0%" stop-color="#00ffff" stop-opacity="0.5" />
                <stop offset="100%" stop-color="#00ffff" stop-opacity="1" />
            </linearGradient>
            <filter id="blurFilter2">
                <feGaussianBlur in="SourceGraphic" stdDeviation="4" result="blur1" />
                <feMerge>
                    <feMergeNode in="blur1" />
                    <feMergeNode in="blur1" />
                    <feMergeNode in="blur1" />
                    <feMergeNode in="blur1" />
                    <feMergeNode in="SourceGraphic" />
                </feMerge>
            </filter>
            <filter id="dropshadow" height="200%" width="200%" x="-50%" y="-50%">
                <feGaussianBlur in="SourceAlpha" stdDeviation="20"/>
                <feOffset dx="0" dy="0" result="offsetblur"/>
                <feMerge>
                    <feMergeNode/>
                    <feMergeNode in="SourceGraphic"/>
                </feMerge>
            </filter>
        </defs>
        <g id="fheight_logo" style="overflow:visible; filter: url(#blurFilter2);">
            <!-- F -->
            <path class="draw-line" d="M5 10 L5 74 L18 74 L18 48 L45 48 L45 36 L18 36 L18 22 L50 22 L50 10 Z"/>
